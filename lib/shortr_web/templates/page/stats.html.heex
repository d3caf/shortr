<table class="table table-dark">
  <thead>
    <tr>
      <th>Original</th>
      <th>Shortened</th>
      <th>Hits</th>
    </tr>
  </thead>
  <tbody>
    <%= for %{url: url, hits: hits} = link <- @links do %>
    <tr>
      <td><%= url %></td>
      <td><%= link_url(link) %></td>
      <td><%= hits %></td>
    </tr>
    <% end %>
  </tbody>
</table>
